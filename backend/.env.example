# Server Configuration
NODE_ENV=production
PORT=8080
HOST=0.0.0.0

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d
OTP_EXPIRES_IN=600000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Google Cloud
GCP_PROJECT_ID=your-gcp-project-id
GCP_DATASET_ID=progress_tracker
GCP_LOCATION=northamerica-northeast2

# BigQuery
BIGQUERY_DATASET=progress_tracker
BIGQUERY_TABLE_SURVEY=survey_data
BIGQUERY_TABLE_ANALYTICS=analytics_results
BIGQUERY_TABLE_JOURNEYS=patient_journeys

# Gemini AI
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-pro

# Redis (optional, for production)
REDIS_URL=redis://localhost:6379
REDIS_TTL=3600

# Email Backend Service
EMAIL_SERVICE_URL=https://email-backend-1062713216421.northamerica-northeast2.run.app
EMAIL_SERVICE_TIMEOUT=30000

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=.csv,.xlsx,.xls

# Logging
LOG_LEVEL=info
LOG_DIR=logs

# Session
SESSION_SECRET=your-session-secret-change-this
SESSION_TIMEOUT=3600000
